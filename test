    transition: opacity 1s ease-in-out;
  }

  .opening-screen.hidden {
    opacity: 0;
    pointer-events: none;
  }

  .center-content {
    text-align: center;
    color: #fff;
    z-index: 10000;
  }

  /* Customizing Button */
  .open-btn {
    font-family: 'Outfit', sans-serif;
    font-size: 1.5rem;
    font-weight: normal;
    padding: 0.6rem 1.5rem;
    background-color: #790001;
    color: #fff;
    border: none;
    border-radius: 10px;
