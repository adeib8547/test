        <script>
document.addEventListener('DOMContentLoaded', function () {
    const modal = document.getElementById('trulyinvite-rsvp-modal');
    const openButton = document.getElementById('trulyinvite-rsvp-button');
    const closeButton = document.getElementById('trulyinvite-close-modal');
    const form = document.getElementById('trulyinvite-rsvp-form');
    const attendanceSelect = document.getElementById('trulyinvite_attendance');
    const countContainer = document.getElementById('attendee-count-container');
    const countInput = document.getElementById('trulyinvite_attendee_count');

    //  Dapatkan max attendee dari PHP terus
    const maxAttendee = 10;

    if (countInput) {
        countInput.setAttribute('max', maxAttendee);
    }

    openButton.addEventListener('click', function () {
        modal.style.display = 'flex';
    });

    closeButton.addEventListener('click', function () {
        modal.style.display = 'none';
    });

    modal.addEventListener('click', function (event) {
        if (event.target === modal) {
            modal.style.display = 'none';
        }
    });

    attendanceSelect.addEventListener('change', function() {
        if (this.value === 'No') {
            countContainer.style.display = 'none';
            countInput.value = '1';
        } else {
            countContainer.style.display = 'block';
        }
    });

    form.addEventListener('submit', function(e) {
        if (attendanceSelect.value === 'No' && parseInt(countInput.value) > 1) {
            e.preventDefault();
            alert('For "Tidak Hadir", the number of attendees should be 1.');
            countInput.value = '1';
            return;
        }

        if (attendanceSelect.value === 'Yes' && parseInt(countInput.value) > maxAttendee) {
            e.preventDefault();
            alert(`Jumlah maksimum kehadiran untuk jemputan ini adalah ${maxAttendee}.`);
            countInput.value = maxAttendee;
        }
    });
});
</script>

        

</div>
						</div>
				</div>
					</div>
		</div>
				<div class="elementor-column elementor-col-33 elementor-top-column elementor-element elementor-element-d89e55e" data-id="d89e55e" data-element_type="column">
