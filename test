					<script>
document.addEventListener('DOMContentLoaded', function() {
  const modal = document.getElementById('trulyinvite-rsvp-modal');

  // Bila klik menu RSVP (#rsvp-section)
  document.querySelectorAll('a[href="#rsvp"]').forEach(link => {
    link.addEventListener('click', function(e) {
      e.preventDefault();

      // Scroll ke section, align ke tengah skrin
      const target = document.querySelector('#rsvp');
      if (target) {
        target.scrollIntoView({ behavior: 'smooth', block: 'center' });

        // Lepas scroll, auto buka modal
        setTimeout(() => {
          if (modal) {
            modal.style.display = 'flex';
          }
        }, 800);
      }
    });
  });
});
</script>
				</div>
				</div>
				<div class="elementor-element elementor-element-90bcdce elementor-widget elementor-widget-html" data-id="90bcdce" data-element_type="widget" data-widget_type="html.default">
				<div class="elementor-widget-container">
					<style>
/* Container for bubbles */
.bubble-container {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    pointer-events: none;
    overflow: hidden;
}

/* Bubble style */
.bubble {
    position: absolute;
    bottom: -50px;
    width: 10px;
    height: 10px;
    background: rgba(255, 255, 255, 0.5); /* Change opacity here */
    border-radius: 50%;
    animation: moveBubble 5s linear infinite;
    opacity: 0.5; /* Adjust the bubble opacity */
}

/* Bubble animation */
@keyframes moveBubble {
    0% {
        transform: translateX(0) translateY(0) scale(1);
        opacity: 0.5;
    }
    50% {
        opacity: 1;
    }
    100% {
        transform: translateX(100px) translateY(-500px) scale(0.5); /* Move right and get smaller */
        opacity: 0;
    }
}
</style>

<div class="bubble-container"></div>

<script>
document.addEventListener("DOMContentLoaded", function () {
    const bubbleContainer = document.querySelector(".bubble-container");

    function createBubble() {
        const bubble = document.createElement("div");
        bubble.classList.add("bubble");

        // Random position and size
        bubble.style.left = Math.random() * window.innerWidth + "px";
        bubble.style.width = Math.random() * 10 + 5 + "px";  // Bubble size
        bubble.style.height = bubble.style.width;

        bubbleContainer.appendChild(bubble);

        // Remove bubble after animation
        setTimeout(() => {
            bubble.remove();
        }, 5000);
    }

    // Generate bubbles every 500ms
    setInterval(createBubble, 500);
});
</script>
				</div>
				</div>
				<div class="elementor-element elementor-element-77c7908 elementor-widget elementor-widget-html" data-id="77c7908" data-element_type="widget" data-widget_type="html.default">
				<div class="elementor-widget-container">
					<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Scroll Down Info</title>
    <style>
        body {
            margin: 0;
            padding: 0;
            height: 100vh; /* Just to enable scrolling */
            background-color: transparent;
            color: white;
            font-family: outfit, sans-serif;
        }
        .scroll-info {
            position: fixed;
            right: 10px;
            bottom: 80px;
            background: rgba(0, 0, 0, 0.4);
            color: rgba(255, 255, 255, 0.8);
            padding: 8px 5px;
            border-radius: 8px;
            border: 0px solid white;
            font-size: 10px;
            display: none;
            text-align: center;
            z-index: 9999; /* Ensure it's in front of everything */
        }
        .emoji {
            display: block;
            font-size: 15px;
            margin-top: 5px;
            animation: moveEmoji 1s infinite alternate;
            color: white; /* Make emoji white */
        }
        @keyframes moveEmoji {
            from {
                transform: translateY(0);
            }
            to {
                transform: translateY(5px);
            }
        }
    </style>
</head>
<body>
    <div class="scroll-info" id="scrollInfo">Scroll <span class="emoji"></span></div>
    
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            setTimeout(() => {
                let scrollInfo = document.getElementById("scrollInfo");
                scrollInfo.style.display = "block";
                setTimeout(() => {
                    scrollInfo.style.display = "none";
                }, 30000); // Disappear after 3 seconds
            }, 10000); // Show after 5 seconds
        });

        let startY = 0;
        window.addEventListener("touchstart", function(event) {
            startY = event.touches[0].clientY;
        });

        window.addEventListener("touchmove", function(event) {
            let currentY = event.touches[0].clientY;
            if (currentY < startY) {
                document.getElementById("scrollInfo").style.display = "none";
            }
        }, { passive: true });
    </script>
</body>
</html>				</div>
				</div>
					</div>
